@model Domain.Entities.Cart

<div class="navbar-right">

    @Html.ActionLink("Order", "Index", "Cart", new {returnUrl=Request.Url.PathAndQuery },new { @class = "btn btn-default navbar btn" })

</div>
<div class="navbar navbar-text navbar-right">
    <b>Your cart</b>
    @Model.lines.Sum(x=>x.Quantity)Books,
    @Model.ComputeTotalValue().ToString("#USD")
    </div>